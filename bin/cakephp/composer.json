{
	
    "name": "Preslog",
	"minimum-stability": "dev",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.cakephp.org"
        },
		{
			"type": "package",
            "package": {
                "name": "ichikaway/Mongodb",
                "version": "master",
                "source": {
                    "url": "https://github.com/sabohoney/cakephp-mongodb",
                    "type": "git",
                    "reference": "mongoclient"
                }
            }		
		}
    ],
    "require": {
        "pear-cakephp/cakephp": ">=2.4.0",
		"ichikaway/Mongodb": "master",
        "zircote/swagger-php": "dev-master",
        "phpword/phpword": "*"
    },
    "config": {
        "vendor-dir": "Vendor/"
    },
    "autoload": {
        "psr-0": {
            "JqlParser": "Vendor/JqlParser"
        },
        "classmap": ["Vendor/HighchartsPHP/"]
    }
}